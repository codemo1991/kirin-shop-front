<view class="content b-t"><mescroll-body vue-id="9834bd54-1" down="{{downOption}}" up="{{upOption}}" ref="__r" data-ref="mescrollRef" data-event-opts="{{[['^init',[['mescrollInit']]],['^down',[['downCallback']]],['^up',[['upCallback']]],['^emptyclick',[['emptyClick']]]]}}" onInit="__e" onDown="__e" onUp="__e" onEmptyclick="__e" onVueInit="__l"><block a:for="{{tabItem}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['showReason',['$0'],[[['tabItem','',index]]]]]]]}}" class="list b-b" onTap="__e"><view class="wrapper"><view class="u-box"><text class="name">{{item.time}}</text><text class="mobile">余额：<font class="price" vue-id="{{('9834bd54-2-'+index)+','+('9834bd54-1')}}" onVueInit="__l">{{item.afterAmount}}</font>元</text></view><view class="address-box"><block a:if="{{item.billType==3||item.billType==2}}"><text class="tagGreen">{{item.billTypeName}}</text></block><block a:else><text class="tagRed">{{item.billTypeName}}</text></block><block a:if="{{item.billType==3||item.billType==2}}"><text class="address">{{"-"+item.balance+" 元"}}</text></block><block a:else><text class="address">{{"+"+item.balance+" 元"}}</text></block></view></view></view></block></mescroll-body></view>