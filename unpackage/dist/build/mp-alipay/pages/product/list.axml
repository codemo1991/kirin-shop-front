<view class="content"><view class="goods-list"><block a:for="{{goodsList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index,'goodId']]]]]]]}}" class="goods-item" onTap="__e"><view class="image-wrapper"><image src="{{item.imgUrl}}" mode="aspectFill"></image></view><text class="title clamp">{{item.goodName}}</text><view class="price-box"><text class="price">{{item.price}}</text></view></view></block></view></view>