<view class="content b-t"><block a:for="{{addressList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['checkAddress',['$0'],[[['addressList','',index]]]]]]]}}" class="list b-b" onTap="__e"><view class="wrapper"><view class="u-box"><text class="name">{{item.name}}</text><text class="mobile">{{item.phone}}</text></view><view class="address-box"><block a:if="{{item.defaultFlag==1}}"><text class="tag">默认</text></block><text class="address">{{item.province+" "+item.city+" "+item.zone+" "+item.address}}</text></view></view><text data-event-opts="{{[['tap',[['addAddress',['edit','$0'],[[['addressList','',index]]]]]]]}}" class="yticon icon-bianji" style="padding-right:10px;" catchTap="__e"></text><text data-event-opts="{{[['tap',[['delAddr',['$0',index],[[['addressList','',index]]]]]]]}}" class="yticon icon-iconfontshanchu1" catchTap="__e"></text></view></block><button data-event-opts="{{[['tap',[['addAddress',['add']]]]]}}" class="add-btn" onTap="__e">新增地址</button></view>