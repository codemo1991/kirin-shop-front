<view class="container"><view class="mp-search-box"><input class="ser-input" type="text" value="输入关键字搜索" disabled="{{true}}"/></view><view class="carousel-section"><view class="titleNview-placing"></view><view class="titleNview-background" style="backgroundColor:rgb(183, 73, 69);"></view><swiper class="carousel" circular="{{true}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" onChange="__e"><block a:for="{{carouselList}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item data-event-opts="{{[['tap',[['navTo',['$0'],[[['carouselList','',index,'url']]]]]]]}}" class="carousel-item" onTap="__e"><image src="{{item.imgUrl}}"></image></swiper-item></block></swiper><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view></view><block a:for="{{menuList}}" a:for-item="item" a:for-index="index" a:key="index"><view><view class="cate-section"><block a:for="{{item}}" a:for-item="item2" a:for-index="index2" a:key="index2"><view data-event-opts="{{[['tap',[['navToListPage',['$0'],[[['menuList','',index],['','',index2,'cateId']]]]]]]}}" class="cate-item" onTap="__e"><image src="{{item2.imgUrl}}"></image><text>{{item2.name}}</text></view></block></view></view></block><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">热销榜</text><text class="tit2">recommend</text></view><text class="yticon icon-you"></text></view><view class="guess-section"><block a:for="{{goodsList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index]]]]]]]}}" class="guess-item" onTap="__e"><view class="image-wrapper"><image src="{{item.imgUrl}}" mode="aspectFill"></image></view><text class="title clamp">{{item.goodName}}</text><text class="price">{{"￥"+item.price}}</text></view></block></view></view>